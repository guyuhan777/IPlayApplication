        android:layout_height="wrap_content">
    </com.iplay.iplayapplication.adapter.nActivityAdapter.commonComponent.ActivityTitlePart>
    <com.iplay.iplayapplication.adapter.nActivityAdapter.commonComponent.ActivityVideoPart
